class Solution:
    def reverse(self, x: int) -> int:
        if x>=0:
            r=str(x)
            i=int(r[::-1])
            if i>2147483647:
                return 0
            else:
                return i
        elif x<0:
            p=abs(x)
            r=str(p)
            re=r[::-1]
            i= (-1)*(int(re))
            if i<-2147483648:
                return 0
            else:
                return i
        else:
            return 0
